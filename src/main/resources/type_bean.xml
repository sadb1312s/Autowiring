       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id = "engine" class="com.company.bean.modern.Disel"/>
    <bean id = "oldEngine" class = "com.company.bean.old.SteamEngine" />

    <bean id="ladaV8" class="com.company.bean.modern.Car" scope="prototype" autowire="byType">
        <property name="vin" value="321312312"/>
    </bean>

    <bean id="hogvartsExpress" class="com.company.bean.old.SteamTrain" scope="prototype" autowire="byType">
        <property name="model" value="DRB BR 50"/>
    </bean>
</beans>